local Players = game:GetService("Players")
local localPlayer = Players.LocalPlayer

local function createGUI()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/privevevetiki-cpu/Adm_PNl/refs/heads/main/gg'))()
end

-- Создаём GUI сразу
createGUI()

-- Пересоздаём GUI при респавне персонажа
localPlayer.CharacterAdded:Connect(function()
    createGUI()
end)
